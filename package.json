{
  "name": "grpc-node-example",
  "version": "1.0.0",
  "description": "GRPC server example",
  "main": "build/index.js",
  "scripts": {
    "start:dev": "npx nodemon",
    "start:server": "ts-node-dev --respawn --transpile-only server/server.ts",
    "start:client": "ts-node-dev --respawn --transpile-only client/app.ts",
    "lint": "eslint . --ext .ts",
    "build:proto": "rm -rf proto-gen && $(npm bin)/proto-loader-gen-types --longs=String --enums=String --defaults ---keepCase --oneofs --grpcLib=@grpc/grpc-js --outDir=src/proto-gen/ proto/*.proto"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lires5/grpc-node-example.git"
  },
  "author": "Luis Revelo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lires5/grpc-node-example/issues"
  },
  "homepage": "https://github.com/lires5/grpc-node-example#readme",
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "eslint": "^8.33.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.8.7",
    "@grpc/proto-loader": "^0.7.4",
    "dotenv": "^16.0.3",
    "express": "^4.18.2"
  }
}
